{
  "name": "@nuxtjs/sentry",
  "version": "7.1.4",
  "description": "Sentry module for Nuxt.js",
  "repository": "nuxt-community/sentry-module",
  "license": "MIT",
  "contributors": [
    {
      "name": "Diederik van den Burger <diederik@webrelated.nl>"
    },
    {
      "name": "Rafal Chlodnicki (@rchl)"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/module.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn prepack",
    "prepack": "nuxt-module-build",
    "dev:prepare": "nuxt-module-build --stub",
    "dev:fixture": "node ./node_modules/nuxt/bin/nuxt.js -c ./test/fixture/default/nuxt.config.cjs",
    "dev:fixture:build": "node ./node_modules/nuxt/bin/nuxt.js build -c ./test/fixture/default/nuxt.config.cjs",
    "dev:fixture:start": "node ./node_modules/nuxt/bin/nuxt.js start -c ./test/fixture/default/nuxt.config.cjs",
    "dev:generate": "nuxt generate -c ./test/fixture/default/nuxt.config.cjs --force-build",
    "analyze": "node ./node_modules/nuxt/bin/nuxt.js build --analyze -c ./test/fixture/default/nuxt.config.js",
    "lint": "eslint --ext .vue,.js,.ts .",
    "lint:fix": "eslint --ext .vue,.js,.ts . --fix",
    "lint:fixture": "eslint --ext .vue,.js --no-ignore 'test/fixture/*/.nuxt/sentry.*'",
    "release": "release-it",
    "test:fixture": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand",
    "test": "yarn prepack && yarn lint && yarn test:fixture && yarn lint:fixture",
    "coverage": "codecov"
  },
  "dependencies": {
    "@sentry/integrations": "^7.41.0",
    "@sentry/node": "^7.41.0",
    "@sentry/vue": "^7.41.0",
    "consola": "^2.0.0",
    "defu": "^6.0.0",
    "hash-sum": "^2.0.0",
    "lodash.mergewith": "^4.6.2",
    "mlly": "^1.0.0",
    "pathe": "^1.0.0",
    "unctx": "^2.0.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "0.2.1",
    "@nuxt/types": "2.16.3",
    "@nuxtjs/eslint-config-typescript": "12.0.0",
    "@nuxtjs/module-test-utils": "1.6.3",
    "@release-it/conventional-changelog": "5.1.1",
    "@sentry/tracing": "7.41.0",
    "@sentry/webpack-plugin": "1.20.0",
    "@types/hash-sum": "1.0.0",
    "@types/jest": "29.5.0",
    "@types/lodash.mergewith": "4.6.7",
    "@types/node": "16.18.16",
    "codecov": "3.8.3",
    "eslint": "8.36.0",
    "eslint-plugin-jest": "27.2.1",
    "hookable": "5.5.1",
    "jest": "29.5.0",
    "nuxt": "2.16.3",
    "playwright-chromium": "1.31.2",
    "release-it": "15.9.0",
    "sentry-testkit": "5.0.5",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "vue": "2.7.14",
    "vuex": "4.1.0"
  }
}
