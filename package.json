{
  "name": "@nuxtjs/sentry",
  "version": "4.1.3",
  "description": "Sentry module for Nuxt.js",
  "repository": "nuxt-community/sentry-module",
  "license": "MIT",
  "contributors": [
    {
      "name": "Diederik van den Burger <diederik@webrelated.nl>"
    },
    {
      "name": "Rafal Chlodnicki (@rchl)"
    }
  ],
  "files": [
    "lib",
    "types"
  ],
  "main": "lib/module.js",
  "scripts": {
    "lint": "eslint --ext .vue,.js lib test types/**/*.ts",
    "lint:fixture": "eslint --ext .vue,.js --no-ignore 'test/fixture/*/.nuxt/sentry.*'",
    "release": "release-it",
    "test:fixture": "jest --runInBand",
    "test": "yarn lint --fix && yarn test:fixture && yarn lint:fixture"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      }
    }
  },
  "dependencies": {
    "@sentry/browser": "^5.18.1",
    "@sentry/integrations": "^5.18.1",
    "@sentry/node": "^5.18.1",
    "@sentry/webpack-plugin": "^1.11.1",
    "consola": "^2.14.0",
    "deepmerge": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@nuxt/types": "^2.13.2",
    "@nuxtjs/eslint-config-typescript": "^2.0.0",
    "@nuxtjs/module-test-utils": "^1.6.2",
    "@release-it/conventional-changelog": "^1.1.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "codecov": "^3.7.0",
    "eslint": "^7.3.1",
    "jest": "^26.1.0",
    "nuxt-edge": "^2.11.1-26378723.c5d0067",
    "release-it": "^13.6.4",
    "typescript": "^3.9.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "types": "types/index.d.ts"
}
